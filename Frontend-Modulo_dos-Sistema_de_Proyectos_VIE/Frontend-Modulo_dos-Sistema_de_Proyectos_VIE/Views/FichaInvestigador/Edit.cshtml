@model Frontend_Modulo_dos_Sistema_de_Proyectos_VIE.Models.FichaInvestigador.FichaInvestigadorViewModel
@{

    ViewBag.Title = "Ficha del Investigador";
    Layout = "~/Views/Shared/MainMenu.cshtml";
}

@using (Html.BeginForm())
{

    <div class="container-fluid p-4 rounded" style="box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19); background-color:white; width:90%">
        <!-- Datos generales -->
        <h3 class="blue font-weight-bold m-2">Información Personal</h3>
        <div class="row">
            <div class="col-12">
                <p style="color:red"><b>Recuerde seleccionar cuales campos desea tener privados <span class="fa fa-lock"></span> , de lo contrario serán públicos.</b></p>
            </div>
            <!-- Columna izquierda -->
            <div class="col-md-6 mt-3">
                <!-- Fecha de ultima modificacion -->
                <div class="row">
                    <div class="col-md-5">
                        <label>
                            Fecha de última modificación
                        </label>
                    </div>
                    <div class="col-md-5">
                        <input class="form-control" placeholder="09/10/2021 14:34:12" disabled />
                    </div>
                </div>

                <!-- Numero de identidad -->
                <div class="row mt-3">
                    <div class="col-md-4 ">
                        @Html.DisplayNameFor(model => model.NumeroIdentidad)
                        @Html.LabelFor(model => model.NumeroIdentidad, new { @class = "control-label " })
                    </div>
                    <div class="col-md-4 ">
                        @Html.EditorFor(model => model.NumeroIdentidad, new { htmlAttributes = new { @class = "form-control " }})
                    </div>
                    <div class="col-md-4">
                        <div class="form-check m-2">
                            <input class="form-check-input" type="checkbox" value="" id="numeroIdentidadPrivacidad">
                            <label class="form-check-label" for="numeroIdentidadPrivacidad">
                                Privado <span class="fa fa-lock" />
                            </label>
                        </div>
                    </div>
                </div>

                <!-- Nombre -->
                <div class="row mt-3">
                    <div class="col-md-4 ">
                        <label>Nombre</label>
                    </div>
                    <div class="col-md-4 ">
                        @Html.TextBoxFor(vista => vista.Nombre, new { @class = "form-control " })
                    </div>
                    <div class="col-md-4">
                        <div class="form-check m-2">
                            <input class="form-check-input" type="checkbox" value="" id="nombrePrivacidad">
                            <label class="form-check-label" for="nombrePrivacidad">
                                Privado <span class="fa fa-lock" />
                            </label>
                        </div>
                    </div>
                </div>

                <!-- Primer apellido -->
                <div class="row mt-3">
                    <div class="col-md-4">
                        <label>Primer apellido</label>
                    </div>
                    <div class="col-md-4">
                        <div class="mb-3">
                            @Html.TextBoxFor(vista => vista.PrimerApellido, new { @class = "form-control " })
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="form-check m-2">
                            <input class="form-check-input" type="checkbox" value="" id="apellido1IdentidadPrivacidad">
                            <label class="form-check-label" for="apellido1IdentidadPrivacidad">
                                Privado <span class="fa fa-lock" />
                            </label>
                        </div>
                    </div>
                </div>

                <!-- Segundo apellido -->
                <div class="row mt-3">
                    <div class="col-md-4">
                        <label>Segundo apellido</label>
                    </div>
                    <div class="col-md-4">
                        @{
                            if (Model.SegundoApellido != null)
                            {
                                Html.TextBoxFor(vista => vista.SegundoApellido, new { @class = "form-control " });
                            }
                            else
                            {
                                <input class="form-control" placeholder="No indica segundo apelldio" />
                            }
                        }
                    </div>
                    <div class="col-md-4">
                        <div class="form-check m-2">
                            <input class="form-check-input" type="checkbox" value="" id="apellido2Privacidad">
                            <label class="form-check-label" for="apellido2Privacidad">
                                Privado <span class="fa fa-lock" />
                            </label>
                        </div>
                    </div>
                </div>

                <!-- Nacionalidad -->
                <div class="row mt-3">
                    <div class="col-md-4 ">
                        <label>Nacionalidad</label>
                    </div>
                    <div class="col-md-4 ">
                        @Html.DropDownListFor(model => model.Nacionalidad.IdNacionalidad, new SelectList(Model.Nacionalidad.CatalogoPais, "Value", "Text", Model.Nacionalidad.IdNacionalidad), htmlAttributes: new { @class = "form-control", id = "Nacionalidad" })
                    </div>
                    <div class="col-md-4">
                        <div class="form-check m-2">
                            <input class="form-check-input" type="checkbox" value="" id="nacionalidadPrivacidad">
                            <label class="form-check-label" for="nacionalidadPrivacidad">
                                Privado <span class="fa fa-lock" />
                            </label>
                        </div>
                    </div>
                </div>

                <!---Pais Nacimiento -->
                <div class="row mt-3">
                    <div class="col-md-4 ">
                        <label>Pais Nacimiento</label>
                    </div>
                    <div class="col-md-4 ">
                        @Html.DropDownListFor(vista => vista.PaisNacimiento.IdPaisNacimiento, new SelectList(Model.PaisNacimiento.CatalogoPais, "Value", "Text"), htmlAttributes: new { @class = "form-control", id = "PaisNacimiento" })
                    </div>
                    <div class="col-md-4">
                        <div class="form-check m-2">
                            <input class="form-check-input" type="checkbox" value="" id="paisNacimientoPrivacidad">
                            <label class="form-check-label" for="paisNacimientoPrivacidad">
                                Privado <span class="fa fa-lock" />
                            </label>
                        </div>
                    </div>
                </div>

                <!-- Fecha Nacimiento -->
                <div class="row mt-3">
                    <div class="col-md-4">
                        <label>Fecha Nacimiento</label>
                    </div>
                    <div class="col-md-4 ">
                        <div class="input-group date" data-provide="datepicker" data-date-format="dd/mm/yyyy">
                            @Html.TextBoxFor(vista => vista.FechaNacimiento, new { @class = "form-control" })
                            <span class="input-group-addon m-1">
                                <button type="button" id="btnFechaNacimiento" class="fa fa-calendar-plus-o btn btn-primary"></button>
                            </span>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="form-check m-2">
                            <input class="form-check-input" type="checkbox" value="" id="fechaNacimientoPrivacidad">
                            <label class="form-check-label" for="fechaNacimientoPrivacidad">
                                Privado <span class="fa fa-lock" />
                            </label>
                        </div>
                    </div>
                </div>

                <!-- Telefono modal -->
                <div class="modal fade" id="telefonosModal" tabindex="-1" role="dialog">
                    <div class="modal-dialog modal-dialog-centered modal-lg modal-dialog-scrollable" role="document">
                        <div class="modal-content">
                            <div class="modal-header">
                                <h5 class="modal-title">Telefonos</h5>
                                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                    <span aria-hidden="true">&times;</span>
                                </button>
                            </div>
                            <div class="modal-body">

                                <div class="col-md-12 table-responsive">
                                    <table id="Telefonos" class="table" cellspacing="0">
                                        <thead>
                                            <tr>
                                                <th>Tipo</th>
                                                <th>Número de teléfono</th>
                                                <th>Privado</th>
                                                <th>Editar</th>
                                                <th>Eliminar</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @foreach (var telefono in Model.Telefonos)
                                            {
                                                <tr>
                                                    <td>
                                                        @Html.DropDownList(telefono.Tipo,
                                                            new SelectList(telefono.CatalogoTipoTelefono, "Value", "Text"),
                                                            htmlAttributes: new { @class = "form-control" })
                                                    </td>
                                                    <td>@Html.TextBox("NumeroTelefono", telefono.Numero, htmlAttributes: new { @class = "form-control" })</td>
                                                    <td>
                                                        <div class="form-check m-2">
                                                            <input class="form-check-input" type="checkbox" value="" id="telefonoPrivacidad">
                                                        </div>
                                                    </td>
                                                    <td>
                                                        <button type="button" class="btnGrande btnAzul">Editar</button>
                                                    </td>
                                                    <td>
                                                        <button type="button" class="btn btn-danger">Eliminar</button>
                                                    </td>
                                                </tr>
                                            }
                                        </tbody>
                                    </table>
                                </div>
                                <hr />
                                <div class="row">
                                    <div class="col-4">
                                        @Html.DropDownList("Tipos de teléfono", Model.CatalogoTipoTelefono, htmlAttributes: new { @class = "form-control", id = "TipoTelefonoNuevo" })
                                    </div>
                                    <div class="col-8">
                                        <input class="form-control" placeholder="Número" />
                                    </div>
                                </div>
                                <div class="row d-flex flex-row-reverse">
                                    <button type="button" class="btnAzul m-3 ">Agregar teléfono</button>
                                </div>
                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-primary">Guardar cambios</button>
                                <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- Telefonos -->
                <div class="row mt-3">
                    <div class="col-md-5">
                        <label>Teléfonos</label>
                    </div>
                    <div class="col-md-5">
                        <button type="button" id="btnVerTelefonos" class="btnAzul btnGrande" data-toggle="modal" data-target="#telefonosModal">Ver teléfonos</button>
                    </div>
                </div>

                <!-- Unidad academica modal -->
                <div class="modal" id="unidadesAcademicasModal" tabindex="-1" role="dialog">
                    <div class="modal-dialog" role="document">
                        <div class="modal-content">
                            <div class="modal-header">
                                <h5 class="modal-title">Unidades académicas</h5>
                                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                    <span aria-hidden="true">&times;</span>
                                </button>
                            </div>
                            <div class="modal-body">

                                <table id="Unidades" class="table" cellspacing="0">
                                    <thead>
                                        <tr>
                                            <th>Unidad académica</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var unidadAcademica in Model.UnidadesAcademicas)
                                        {
                                            <tr>
                                                <td>
                                                    @Html.DropDownList(unidadAcademica.IdUnidadAcademica.ToString(),
                         new SelectList(unidadAcademica.CatalogoUnidadAcademica, "Value", "Text"),
                         htmlAttributes: new { @class = "form-control", id = "UnidadAcademica" })
                                                </td>
                                            </tr>
                                        }
                                    </tbody>
                                </table>

                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-primary">Guardar cambios</button>
                                <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- Unidades academicas -->
                <div class="row mt-3">
                    <div class="col-md-5">
                        <label>Unidades académicas en las que labora </label>
                    </div>
                    <div class="col-md-5">
                        <button type="button" class="btnAzul btnGrande" data-toggle="modal" data-target="#unidadesAcademicasModal">Ver unidades académicas</button>
                    </div>
                </div>

            </div>

            <!-- Columna derecha -->
            <div class="col-md-6 mt-3">

                <!-- Foto -->
                <div class="row">
                    <div class="col-md-12">
                        <label>Foto</label>
                    </div>
                    @{
                        var base64 = Convert.ToBase64String(Model.Fotografia);
                        var imgSrc = String.Format("data:image/gif;base64,{0}", base64);
                    }
                    <div class="col-md-4">
                        <img src="@imgSrc" class="rounded" alt="Imagen del investigador" width="200" height="200">
                    </div>
                    <div class="col-md-4">
                        <div class="wrapperBotonAgregar">
                            <input class="btnGrande btnAzul" type="button" value="Seleccionar foto">
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="form-check m-2">
                            <input class="form-check-input" type="checkbox" value="" id="fotoPrivacidad">
                            <label class="form-check-label" for="fotoPrivacidad">
                                Privado <span class="fa fa-lock" />
                            </label>
                        </div>
                    </div>
                </div>

                <!-- Fecha de ingreso al TEC -->
                <div class="row mt-3">
                    <div class="col-md-4 ">
                        <label>Fecha de Ingreso al TEC</label>
                    </div>
                    <div class="col-md-4 ">
                        <div id="datepicker-group" class="input-group date" data-date-format="dd/mm/yyyy">
                            @Html.TextBoxFor(vista => vista.FechaDeIngresoInstitucion, new { @class = "form-control" })
                            <span class="input-group-addon"><i class="glyphicon glyphicon-calendar"></i></span>
                        </div>
                    </div>
                    <div class="col-md-4">
                        <div class="form-check m-2">
                            <input class="form-check-input" type="checkbox" value="" id="fechaIngresoPrivacidad">
                            <label class="form-check-label" for="fechaIngresoPrivacidad">
                                Privado <span class="fa fa-lock" />
                            </label>
                        </div>
                    </div>
                </div>

                <!-- Seudonimo bibliografico -->
                <div class="row mt-3">
                    <div class="col-md-4 ">
                        <label>Seudónimo Bibliográfico</label>
                    </div>
                    <div class="col-md-4">
                        @Html.TextBoxFor(vista => vista.Seudonimo, new { @class = "form-control " })
                    </div>
                    <div class="col-md-4">
                        <div class="form-check m-2">
                            <input class="form-check-input" type="checkbox" value="" id="seudonimoPrivacidad">
                            <label class="form-check-label" for="seudonimoPrivacidad">
                                Privado <span class="fa fa-lock" />
                            </label>
                        </div>
                    </div>
                </div>

                <!-- Correo modal -->
                <div class="modal fade" id="correosModal" tabindex="-1" role="dialog">
                    <div class="modal-dialog modal-dialog-centered modal-lg modal-dialog-scrollable" role="document">
                        <div class="modal-content">
                            <div class="modal-header">
                                <h5 class="modal-title">Correos</h5>
                                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                    <span aria-hidden="true">&times;</span>
                                </button>
                            </div>
                            <div class="modal-body">

                                <div class="col-md-12 table-responsive">
                                    <table id="Correos" class="table" cellspacing="0">
                                        <thead>
                                            <tr>
                                                <th>Correo</th>
                                                <th>Privado</th>
                                                <th>Editar</th>
                                                <th>Eliminar</th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                            @foreach (var correo in Model.Correos)
                                            {
                                                <tr>
                                                    <td>@Html.TextBox("Correo", correo.Correo, htmlAttributes: new { @class = "form-control" })</td>
                                                    <td>
                                                        <div class="form-check m-2">
                                                            <input class="form-check-input" type="checkbox" value="" id="correoPrivacidad">
                                                        </div>
                                                    </td>
                                                    <td>
                                                        <button type="button" class="btnGrande btnAzul">Editar</button>
                                                    </td>
                                                    <td>
                                                        <button type="button" btn btn-danger">Eliminar</button>
                                                    </td>
                                                </tr>
                                            }
                                        </tbody>
                                    </table>
                                </div>
                                <hr />
                                <div class="row">
                                    <input class="form-control" placeholder="Correo electrónico" />
                                </div>
                                <div class="row d-flex flex-row-reverse">
                                    <button type="button" class="btnAzul m-3 ">Agregar correo</button>
                                </div>
                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-primary">Guardar cambios</button>
                                <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- Correos -->
                <div class="row mt-3">
                    <div class="col-md-5">
                        <label>Correos</label>
                    </div>
                    <div class="col-md-5">
                        <button type="button" id="btnVerCorreos" class="btnAzul btnGrande" data-toggle="modal" data-target="#correosModal">Ver correos</button>
                    </div>
                </div>

                <!-- Apartado postal-->
                <div class="row mt-3">
                    <div class="col-md-4 ">
                        <label>Apartado Postal</label>
                    </div>
                    <div class="col-md-4 ">
                        @Html.TextBoxFor(vista => vista.ApartadoPostal, new { @class = "form-control " })
                    </div>
                    <div class="col-md-4">
                        <div class="form-check m-2">
                            <input class="form-check-input" type="checkbox" value="" id="apartadoPostalPrivacidad">
                            <label class="form-check-label" for="apartadoPostalPrivacidad">
                                Privado <span class="fa fa-lock" />
                            </label>
                        </div>
                    </div>
                </div>

                <!-- Sexo -->
                <div class="row mt-3">
                    <div class="col-md-4 ">
                        <label>Sexo</label>
                    </div>
                    <div class="col-md-4 ">
                        @Html.DropDownListFor(vista => vista.Sexo.Sexo,
                               new SelectList(Model.Sexo.CatalogoSexo, "Value", "Text"),
                               htmlAttributes: new { @class = "form-control", id = "Sexo" })
                    </div>
                    <div class="col-md-4">
                        <div class="form-check m-2">
                            <input class="form-check-input" type="checkbox" value="" id="sexoPrivacidad">
                            <label class="form-check-label" for="sexoPrivacidad">
                                Privado <span class="fa fa-lock" />
                            </label>
                        </div>
                    </div>
                </div>

                <!-- Situacion laboral -->
                <div class="row mt-3">
                    <div class="col-md-4 ">
                        <label>Situación Laboral</label>
                    </div>
                    <div class="col-md-4 ">
                        @Html.DropDownListFor(vista => vista.SituacionLaboral.IdSituacionLaboral,
                               new SelectList(Model.SituacionLaboral.CatalogoSituacionLaboral, "Value", "Text"),
                               htmlAttributes: new { @class = "form-control", id = "SituacionLaboral" })
                    </div>
                    <div class="col-md-4">
                        <div class="form-check m-2">
                            <input class="form-check-input" type="checkbox" value="" id="situacionLaboralPrivacidad">
                            <label class="form-check-label" for="situacionLaboralPrivacidad">
                                Privado <span class="fa fa-lock" />
                            </label>
                        </div>
                    </div>
                </div>

                <!-- Porcentaje Nombramiento -->
                <div class="row mt-3">
                    <div class="col-md-4 ">
                        <label>Porcentaje de Nombramiento Indefinido</label>
                    </div>
                    <div class="col-md-4 ">
                        @Html.DropDownListFor(vista => vista.PorcentajeNombramiento.PorcentajeNombramiento,
                               new SelectList(Model.PorcentajeNombramiento.CatalogoPorcentajeNombramiento, "Value", "Text"),
                               htmlAttributes: new { @class = "form-control", id = "PorcentajeNombramiento" })
                    </div>
                    <div class="col-md-4">
                        <div class="form-check m-2">
                            <input class="form-check-input" type="checkbox" value="" id="situacionLaboralPrivacidad">
                            <label class="form-check-label" for="situacionLaboralPrivacidad">
                                Privado <span class="fa fa-lock" />
                            </label>
                        </div>
                    </div>
                </div>

                <!-- Categoria academica modal -->
                <div class="modal fade" id="categoriasAcademicasModal" tabindex="-1" role="dialog">
                    <div class="modal-dialog modal-lg" role="document">
                        <div class="modal-content">
                            <div class="modal-header">
                                <h5 class="modal-title">Categoria Académica</h5>
                                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                    <span aria-hidden="true">&times;</span>
                                </button>
                            </div>
                            <div class="modal-body">
                                <table class="table table-sm table-borderless">
                                    <thead>
                                        <tr>
                                            <th>
                                                Categoría Académica
                                            </th>
                                            <th>
                                                Privacidad
                                            </th>
                                            <th>
                                                Editar
                                            </th>
                                            <th>
                                                Eliminar
                                            </th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        @foreach (var categoria in Model.CategoriasAcademicas)
                                        {
                                            <tr>
                                                <td>
                                                    @Html.DropDownList(categoria.NombreCategoria.ToString(),
                                new SelectList(categoria.CatalogoCategoriaAcademica, "Value", "Text"),
                                htmlAttributes: new { @class = "form-control", id = "CategoriaAcademica" })
                                                </td>
                                                <td>
                                                    <div class="form-check m-2">
                                                        <input class="form-check-input" type="checkbox" value="" id="correoPrivacidad">
                                                    </div>
                                                </td>
                                                <td>
                                                    <button type="button" class="btnGrande btnAzul">Editar</button>
                                                </td>
                                                <td>
                                                    <button  type="button" class="btn btn-danger">Eliminar</button>
                                                </td>
                                            </tr>
                                        }
                                    </tbody>
                                </table>
                                <hr />
                                <div class="row">
                                    <input class="form-control" placeholder="Debe ser un catálogo" />
                                </div>
                                <div class="row d-flex flex-row-reverse">
                                    <button type="button" class="btnAzul m-3 ">Agregar categoría académica</button>
                                </div>
                            </div>
                            <div class="modal-footer">
                                <button type="button" class="btn btn-primary">Guardar cambios</button>
                                <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
                            </div>
                        </div>
                    </div>
                </div>

                <!-- Categorias academicas -->
                <div class="row mt-3">
                    <div class="col-md-5 ">
                        <label>Categoría Académica</label>
                    </div>
                    <div class="col-md-5">
                        <button type="button" id="btnVerCategoriasAcademicas" class="btnAzul btnGrande" data-toggle="modal" data-target="#categoriasAcademicasModal">Ver categorías académicas</button>
                    </div>
                </div>

                <!-- Disciplinas cientificas-->
                <div class="row mt-3">
                    <div class="col-md-5 ">
                        <label>Disciplina</label>
                    </div>
                    <div class="col-md-5 ">
                        <div class="wrapperBotonAgregar">
                            <input class="btnGrande btnAzul" type="button" value="Agregar Disciplina">
                        </div>
                    </div>
                </div>

            </div>

        </div>
    </div>

    <!-- OTRAS SECCIONES -->

    <!-- Perfiles académicos -->
    <div class="container-fluid mt-5 p-4 rounded" style="box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19); background-color:white">
        <h3 class="blue font-weight-bold">Perfiles Académicos</h3>

        <!-- Tabla de perfiles académicos -->
        <div class="row align-items-center justify-items-center mt-5">
            <table class="table table-striped ">
                <thead class="table-heading">
                    <tr id="ROW1">
                        <th>Tipo de perfil</th>
                        <th>Dirección</th>
                        <th>Editar</th>
                        <th>Eliminar</th>
                    </tr>
                </thead>
                <tbody>

                    @{
