    <!-- Software -->
    <div class="container-fluid mt-5 mb-5 p-4 rounded" style="box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19); background-color:white">
        <h3 class="blue font-weight-bold">Software</h3>
        <div class="row align-items-center justify-items-center mt-3">
            <div class="col-md-6 form-inline">
                <label class="col-md-6" for="tituloInput">Título</label>
                <input type="text" class="col-md-6 form-control" id="tituloInput" />
            </div>
            <div class="col-md-6 form-inline">
                <label class="col-md-6" for="plataformaInput">Plataforma</label>
                <input type="text" class="col-md-6 form-control" id="licenciadaAInput" />
            </div>
        </div>
        <div class="row align-items-center justify-items-center mt-3">
            <div class="col-md-6 form-inline">
                <label class="col-md-6">Año</label>
                <input type="text" class="col-md-6 form-control" id="tituloArticuloInput" />
            </div>
        </div>
        <div class="row align-items-center justify-items-center mt-3">
            <div class="col-md-3 form-inline">
                <div class="wrapperBotonAgregar">
                    <input class="btnGrande btnAzul" type="button" value="Agregar Software">
                </div>
            </div>
        </div>
        <div class="row align-items-center justify-items-center mt-5">
            <table class="table table-striped ">
                <thead class="table-heading">
                    <tr id="ROW1">
                        <th>Título</th>
                        <th>Plataforma</th>
                        <th>Año</th>
                        <th>Editar</th>
                        <th>Eliminar</th>
                    </tr>
                </thead>
                <tbody>
                    @{
                        int contadorSoftware = 0;

                        foreach (var software in Model.Software)
                        {
                            <tr>
                                <td>
                                    @software.Titulo
                                </td>
                                <td>
                                    @software.Plataforma
                                </td>
                                <td>
                                    @software.Ano
                                </td>
                                <td>
                                    <button type="button" class="btnAzul" id="btnEditarSoftware" data-toggle="modal" data-target="#softwareModal-@contadorSoftware">Editar</button>
                                </td>
                                <td>
                                    <button type="button" class="btn btn-danger" id="btnEliminarSoftware">Eliminar</button>
                                </td>
                            </tr>

                            <!-- Modal de idiomas -->
                            <div class="modal fade" id="softwareModal-@contadorSoftware" tabindex="-1" role="dialog">
                                <div class="modal-dialog modal-dialog-centered" role="document">
                                    <div class="modal-content">
                                        <div class="modal-header">
                                            <h5 class="modal-title">Editar Software</h5>
                                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                <span aria-hidden="true">&times;</span>
                                            </button>
                                        </div>
                                        <div class="modal-body">
                                            <div class="mx-2 my-2">
                                                <label>Título</label>
                                            </div>
                                            <div class="mx-2 my-2">
                                                @Html.TextBox("titulo" + contadorSoftware, software.Titulo, htmlAttributes: new { @class = "form-control col" })
                                            </div>

                                            <div class="mx-2 my-2">
                                                <label>Licenciada a</label>
                                            </div>
                                            <div class="mx-2 my-2">
                                                @Html.TextBox("plataforma" + contadorSoftware, software.Plataforma, htmlAttributes: new { @class = "form-control col" })
                                            </div>

                                            <div class="mx-2 my-2">
                                                <label>Año</label>
                                            </div>
                                            <div class="mx-2 my-2">
                                                @Html.TextBox("ano" + contadorSoftware, software.Ano, htmlAttributes: new { @class = "form-control col" })
                                            </div>

                                        </div>
                                        <div class="modal-footer">
                                            <button type="button" class="btnGrande btnAzul">Guardar Software</button>
                                            <button type="button" class="btn btn-danger" data-dismiss="modal">Cancelar</button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            contadorSoftware++;
                        }
                    }
                </tbody>
            </table>
        </div>
    </div>

    <!-- Ponencias -->
    <div class="container-fluid p-4 rounded" style="box-shadow: 0 4px 8px 0 rgba(0, 0, 0, 0.2), 0 6px 20px 0 rgba(0, 0, 0, 0.19); background-color:white">
        <h3 class="blue font-weight-bold">Ponencias</h3>
        <div class="row align-items-center justify-items-center mt-3">
            <div class="col-md-6 form-inline">
                <label class="col-md-6" for="nombrePatenteInput">Título</label>
                <input type="text" class="col-md-6 form-control" id="tituloArticuloInput" />
            </div>
            <div class="col-md-6 form-inline">
                <label class="col-md-6" for="numPatenteInput">Seminario o conferencia</label>
                <input type="text" class="col-md-6 form-control" id="tituloArticuloInput" />
            </div>
        </div>
        <div class="row align-items-center justify-items-center mt-3">
            <div class="col-md-6 form-inline">
                <label class="col-md-6" for="inscritoEnInput">Año</label>
                <input type="text" class="col-md-6 form-control" id="tituloArticuloInput" />
            </div>
            <div class="col-md-6 form-inline">
                <label class="col-md-6">País</label>
                @Html.DropDownList("catalogoPais_EstudioRealizado", new SelectList(Model.CatalogoPais, "Value", "Text"), htmlAttributes: new { @class = "form-control col-md-6 form-inline" })
            </div>
        </div>
        <div class="row align-items-center justify-items-center mt-3">
            <div class="col-md-3 form-inline">
                <div class="wrapperBotonAgregar">
                    <input class="btnGrande btnAzul" type="button" value="Agregar artículo">
                </div>
            </div>
        </div>
        <div class="row align-items-center justify-items-center mt-5">
            <table class="table table-striped ">
                <thead class="table-heading">
                    <tr id="ROW1">
                        <th>Título</th>
                        <th>Seminario o conferencia</th>
                        <th>Año</th>
                        <th>País</th>
                        <th>Editar</th>
                        <th>Eliminar</th>
                    </tr>
                </thead>
                <tbody>
                    @{
                        int contadorPonencias = 0;

                        foreach (var ponencia in Model.Ponencias)
                        {
                            <tr>
                                <td>
                                    @ponencia.Titulo
                                </td>
                                <td>
                                    @ponencia.Seminario
                                </td>
                                <td>
                                    @ponencia.Ano
                                </td>
                                <td>
                                    @ponencia.NombrePais
                                </td>
                                <td>
                                    <button type="button" class="btnAzul" id="btnEditarPonencias" data-toggle="modal" data-target="#ponenciasModal-@contadorPonencias">Editar</button>
                                </td>
                                <td>
                                    <button type="button" class="btn btn-danger" id="btnEliminarPonencias">Eliminar</button>
                                </td>
                            </tr>

                            <!-- Modal de idiomas -->
                            <div class="modal fade" id="ponenciasModal-@contadorPonencias" tabindex="-1" role="dialog">
                                <div class="modal-dialog modal-dialog-centered" role="document">
                                    <div class="modal-content">
                                        <div class="modal-header">
                                            <h5 class="modal-title">Editar Ponencias</h5>
                                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                                <span aria-hidden="true">&times;</span>
                                            </button>
                                        </div>
                                        <div class="modal-body">
                                            <div class="mx-2 my-2">
                                                <label>Título</label>
                                            </div>
                                            <div class="mx-2 my-2">
                                                @Html.TextBox("titulo" + contadorPonencias, ponencia.Titulo, htmlAttributes: new { @class = "form-control col" })
                                            </div>

                                            <div class="mx-2 my-2">
                                                <label>Seminario o conferencia</label>
                                            </div>
                                            <div class="mx-2 my-2">
                                                @Html.TextBox("semConf" + contadorPonencias, ponencia.Seminario, htmlAttributes: new { @class = "form-control col" })
                                            </div>

                                            <div class="mx-2 my-2">
                                                <label>Año</label>
                                            </div>
                                            <div class="mx-2 my-2">
                                                @Html.TextBox("ano" + contadorPonencias, ponencia.Ano, htmlAttributes: new { @class = "form-control col" })
                                            </div>

                                            <div class="mx-2 my-2">
                                                <label>País</label>
                                            </div>
                                            <div class="mx-2 my-2">
                                                @Html.TextBox("pais" + contadorPonencias, ponencia.NombrePais, htmlAttributes: new { @class = "form-control col" })
                                            </div>
                                        </div>
                                        <div class="modal-footer">
                                            <button type="button" class="btnGrande btnAzul">Guardar Ponencias</button>
                                            <button type="button" class="btn btn-danger" data-dismiss="modal">Cancelar</button>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            contadorPonencias++;
                        }
                    }
                </tbody>
            </table>
        </div>
    </div>

}
