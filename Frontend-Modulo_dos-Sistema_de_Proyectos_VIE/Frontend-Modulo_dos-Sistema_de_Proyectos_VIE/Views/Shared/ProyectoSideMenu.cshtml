<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/SeguimientoProyecto.css")
    @Scripts.Render("~/bundles/modernizr")
</head>

<body>


    <nav class="navbar navbar-inverse navbar-fixed-top">
        <div class="container-fluid">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand" href="#">Project name</a>
            </div>
            <div id="navbar" class="navbar-collapse collapse">
                <ul class="nav navbar-nav navbar-right">
                    <li><a href="#">Dashboard</a></li>
                    <li><a href="#">Settings</a></li>
                    <li><a href="#">Profile</a></li>
                    <li><a href="#">Help</a></li>
                </ul>
                <form class="navbar-form navbar-right">
                    <input type="text" class="form-control" placeholder="Search...">
                </form>
            </div>
        </div>
    </nav>
    <div class="page-title">
        <h3>Datos del proyecto</h3>
    </div>
    <div class="col-sm-3 col-md-2 sidebar sideMenu">
        <ul class="nav nav-sidebar">
            <li class="active"><a href="#">Datos del proyecto <span class="sr-only">(current)</span></a></li>
            <li><a href="#">Investigadores Asociados</a></li>
            <li><a href="#">Cambiar investigador coordinador</a></li>
            <li><a href="#">Objetivos de Desarrollo Sostenible</a></li>
            <li><a href="#">Áreas Franscati</a></li>
            <li><a href="#">Población beneficiaria</a></li>
            <li><a href="#">Ubicación geográfica</a></li>
            <li><a href="#">Palabras claves</a></li>
            <li><a href="#">Presupuesto</a></li>
            <li><a href="#">Departamentos</a></li>
            <li><a href="#">Programación de informes</a></li>
            <li><a href="#">Ampliar fechas</a></li>
            <li><a href="#">Ingresar informes</a></li>
            <li><a href="#">Compras</a></li>
            <li><a href="#">Contratos</a></li>
            <li><a href="#">Cronograma</a></li>
            <li><a href="#">Incidencias</a></li>
            <li><a href="#">Bitacoras</a></li>
        </ul>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>
</body>
</html>
